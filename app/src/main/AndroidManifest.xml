<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.icem_placement">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".student.WebActivity"></activity>
        <activity android:name=".student.ViewAnswersActivity" />
        <activity android:name=".student.AnswerSubmitActivity" />
        <activity android:name=".student.QuestionSubmitActivity" />
        <activity android:name=".admin.AdminMainActivity" />
        <activity android:name=".chat.MessageActivity" />
        <activity android:name=".chat.ChatActivity" />
        <activity android:name=".student.MainActivity" />
        <activity android:name=".registration.SignUp" />
        <activity android:name=".registration.SignIn" />
        <activity android:name=".registration.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>